{
  "name": "circular_buffer_js",
  "version": "0.4.1",
  "description": "TS implementation of a circular buffer and also a JS compile-down. Well tested (which is the point)",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf -f dist && mkdir dist && cd docs && cd docs && rimraf -f current && mkdir current && cd .. && cd ..",
    "dist": "cp build/es6/circular_buffer.d.ts dist/",
    "docs": "typedoc --out docs/docs/current src/ts && node src/build_js/archive_docs_by_version.js",
    "site": "cp -r src/html/* docs/",
    "version": "node ./src/build_js/write_version_file.js",
    "typescript": "tsc --build tsconfig.json",
    "build": "npm run clean && npm run version && npm run typescript && npm run docs && npm run dist",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/StoneCypher/circular_buffer_js.git"
  },
  "keywords": [
    "circular",
    "buffer",
    "circular-buffer",
    "ring",
    "buffer",
    "ring-buffer",
    "queue",
    "circular-queue",
    "ring-queue",
    "stonecypher"
  ],
  "author": "John Haugeland <stonecypher@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/StoneCypher/circular_buffer_js/issues"
  },
  "homepage": "https://github.com/StoneCypher/circular_buffer_js#readme",
  "devDependencies": {
    "rimraf": "^3.0.2",
    "typedoc": "^0.19.2",
    "typescript": "^4.1.3"
  }
}
